{
    "name": "wemxo/dynamic-form-bundle",
    "description": "Useful symfony bundle that offers the possibility to create dynamic form types based on a given configuration.",
    "minimum-stability": "stable",
    "license": "MIT",
    "type": "symfony-bundle",
    "prefer-stable": true,
    "keywords": [
        "wemxo",
        "dynamic",
        "form",
        "bundle"
    ],
    "authors": [
        {
            "name": "Abdelhak OUADDI [zta9taw]",
            "email": "abdelhak.freelance@gmail.com",
            "homepage": "https://github.com/zta9taw"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^5.0|^6.0",
        "symfony/form": "^5.0|^6.0",
        "symfony/finder": "^5.0|^6.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/validator": "^5.0|^6.0",
        "symfony/phpunit-bridge": "^5.0|^6.0",
        "symfony/cache": "^5.0|^6.0",
        "symfony/yaml": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Wemxo\\DynamicFormBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wemxo\\DynamicFormBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
